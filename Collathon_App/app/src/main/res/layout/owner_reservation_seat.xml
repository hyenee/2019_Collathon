<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.collathon.jamukja.owner.Seat.Owner_list_seat">


        <TextView
            android:id="@+id/et_shopID"
            android:layout_width="97dp"
            android:layout_height="34dp"
            android:layout_marginStart="45dp"
            android:layout_marginTop="32dp"
            android:text="TextView"
            android:textSize="25dp"
            android:gravity="center"
            app:layout_constraintEnd_toStartOf="@+id/textView6"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="55dp"
            android:fontFamily="@font/typo"

            android:text="님의 가게입니다."
            android:textAlignment="center"
            android:textColor="#EE7764"
            android:textSize="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recycler_store"
            android:layout_width="409dp"
            android:layout_height="665dp"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/et_shopID"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="1dp" />


</androidx.constraintlayout.widget.ConstraintLayout>
