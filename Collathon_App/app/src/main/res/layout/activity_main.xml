<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#ffffff"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/category_icon"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:paddingTop="@dimen/spacing_unit_normal"
        android:src="@drawable/product_icon"
        app:layout_constraintDimensionRatio="16:10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
    <!-- app:layout_constraintTop_toBottomOf="@id/category_app_bar_layout" -->


    <ImageButton
        android:id="@+id/category_button_rice"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/spacing_unit_wide"
        android:layout_marginEnd="0dp"
        android:padding="0dp"
        android:scaleType="centerInside"
        android:src="@drawable/button1_rice"
        app:elevation="0dp"
        app:layout_constraintBottom_toTopOf="@id/category_button_meat"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toStartOf="@id/category_button_noodle"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/category_icon"
        app:layout_constraintVertical_chainStyle="packed"/>

    <ImageButton
        android:id="@+id/category_button_noodle"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:padding="0dp"
        android:scaleType="centerInside"
        android:src="@drawable/button2_noodle"
        app:elevation="0dp"
        app:layout_constraintBottom_toTopOf="@id/category_button_dessert"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toStartOf="@id/category_button_chicken"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toEndOf="@id/category_button_rice"
        app:layout_constraintTop_toBottomOf="@id/category_icon"
        app:layout_constraintVertical_chainStyle="packed"/>

    <ImageButton
        android:id="@+id/category_button_chicken"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="@dimen/spacing_unit_wide"
        android:padding="0dp"
        android:scaleType="centerInside"
        android:src="@drawable/button3_chicken"
        app:elevation="0dp"
        app:layout_constraintBottom_toTopOf="@id/category_button_sushi"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toEndOf="@id/category_button_noodle"
        app:layout_constraintTop_toBottomOf="@id/category_icon"
        app:layout_constraintVertical_chainStyle="packed"/>

    <ImageButton
        android:id="@+id/category_button_meat"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/spacing_unit_wide"
        android:layout_marginEnd="0dp"
        android:padding="0dp"
        android:scaleType="centerInside"
        android:src="@drawable/button4_meat"
        app:elevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toStartOf="@id/category_button_dessert"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/category_button_rice"
        app:layout_constraintVertical_chainStyle="packed"/>

    <ImageButton
        android:id="@+id/category_button_dessert"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:padding="0dp"
        android:scaleType="centerInside"
        android:src="@drawable/button5_desert"
        app:elevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toStartOf="@id/category_button_sushi"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toEndOf="@id/category_button_meat"
        app:layout_constraintTop_toBottomOf="@id/category_button_noodle"
        app:layout_constraintVertical_chainStyle="packed"/>

    <ImageButton
        android:id="@+id/category_button_sushi"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="@dimen/spacing_unit_wide"
        android:padding="0dp"
        android:scaleType="centerInside"
        android:src="@drawable/button6_sushi"
        app:elevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toEndOf="@id/category_button_dessert"
        app:layout_constraintTop_toBottomOf="@id/category_button_chicken"
        app:layout_constraintVertical_chainStyle="packed"/>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.1"
            android:orientation="horizontal"></LinearLayout>

        <LinearLayout

            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="bottom"
            android:padding="0dip">

            <Button
                android:id="@+id/homeButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="0dp"
                android:layout_weight="0.1"
                android:fontFamily="@font/nanumsquare"
                android:background="#ffffff "
                android:text="홈"
                app:layout_constraintEnd_toStartOf="@+id/homeButton"
                app:layout_constraintHorizontal_bias="0.37"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="688dp" />

            <Button
                android:id="@+id/pickButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="0dp"
                android:layout_weight="0.1"
                android:fontFamily="@font/nanumsquare"
                android:background="#ffffff"
                android:text="Pick"
                app:layout_constraintEnd_toStartOf="@+id/pickButton"
                app:layout_constraintHorizontal_bias="0.37"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="688dp" />

            <Button
                android:id="@+id/myButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="0dp"
                android:layout_weight="0.1"
                android:fontFamily="@font/nanumsquare"
                android:background="#fffffd"
                android:text="My"
                app:layout_constraintEnd_toStartOf="@+id/myButton"
                app:layout_constraintHorizontal_bias="0.37"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="688dp" />

            <Button
                android:id="@+id/logoutButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="0dp"
                android:layout_weight="0.1"
                android:fontFamily="@font/nanumsquare"
                android:background="#ffffff"
                android:text="로그아웃"
                app:layout_constraintEnd_toStartOf="@+id/logoutButton"
                app:layout_constraintHorizontal_bias="0.37"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="688dp" />
        </LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>