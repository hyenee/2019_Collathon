<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/bottom_navigation_home">

    <fragment
        android:id="@+id/bottom_navigation_home"
        android:name="com.collathon.janolja.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/bottom_navigation_pick"
        android:name="com.collathon.janolja.ui.pick.PickFragment"
        android:label="@string/title_pick"
        tools:layout="@layout/fragment_pick" />

    <fragment
    android:id="@+id/bottom_navigation_my"
    android:name="com.collathon.janolja.ui.my.MyFragment"
    android:label="@string/title_my"
    tools:layout="@layout/fragment_my" />

    <fragment
        android:id="@+id/bottom_navigation_logout"
        android:name="com.collathon.janolja.ui.logout.LogoutFragment"
        android:label="@string/title_logout"
        tools:layout="@layout/fragment_logout" />
</navigation>